{
    "name": "dutchcodingcompany/laravel-json-schema",
    "description": "Adds a service provider for json schemas",
    "keywords": [
        "dutchcodingcompany",
        "dcc",
        "laravel",
        "json",
        "schema"
    ],
    "require": {
        "php" : "^8.0",
        "laravel/framework": "^10.0 | ^11.0 | ^12.0",
        "swaggest/json-schema": "^0.12.33"
    },
    "require-dev": {
        "adamwojs/php-cs-fixer-phpdoc-force-fqcn": "^2.0",
        "friendsofphp/php-cs-fixer": "^3.85"
    },
    "autoload": {
        "psr-4": {
            "DutchCodingCompany\\LaravelJsonSchema\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "DutchCodingCompany\\LaravelJsonSchema\\ServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Tom",
            "email": "tom@dutchcodingcompany.com"
        }
    ],
    "homepage": "https://github.com/dutchcodingcompany",
    "minimum-stability": "dev",
    "prefer-stable": true
}
